{
  "name": "@michaelkramer/eslint-plugin-facepalm",
  "version": "1.1.4",
  "main": "lib/index.js",
  "license": "MIT",
  "description": "All the dumb! code you left in before your push.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "facepalm",
    "comments"
  ],
  "scripts": {
    "test": "mocha tests/**/*.js",
    "patch": "npm version patch",
    "build": "babel plugin -d lib",
    "clean": "rm -r lib",
    "lint": "eslint ./plugin"
  },
  "repository": {
    "url": "git+https://github.com/michaelkramer/eslint-plugin-facepalm.git",
    "type": "git"
  },
  "author": {
    "name": "Michael Kramer",
    "email": "michaelrkramer@gmail.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "babel-minify": "^0.5.1",
    "eslint": "^7.8.1",
    "mocha": "^8.1.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "module": "readonly",
      "require": "readonly",
      "test": "readonly",
      "expect": "readonly"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "no-console": "warn",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": true
        }
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "minify",
        {
          "mangle": true
        }
      ]
    ]
  }
}
