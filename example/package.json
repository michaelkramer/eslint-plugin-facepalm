{
  "name": "facepalm-example",
  "version": "0.0.1",
  "description": "This is an example",
  "main": "index.js",
  "author": {
    "name": "Michael Kramer",
    "email": "michaelrkramer@gmail.com"
  },
  "scripts": {
    "lint": "eslint index.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "eslint": "^7.8.1"
  },
  "dependencies": {
    "eslint-plugin-facepalm": "file:../"
  },
  "babel": {},
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "facepalm"
    ],
    "rules": {
      "facepalm/no-comment-console": "warn",
      "facepalm/no-useless-comments": "warn",
      "facepalm/missing-copyright": [ "warn","./mycopyright.txt" ]
    }
  }
}
